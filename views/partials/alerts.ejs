<div id='alertBox' style='width:100%; text-align:center; font-size:20px; background:rgb(138, 192, 224); color:black'>
    <% if (alerts.error) { %>
        <% alerts.error.forEach(msg => { %>
            <%= msg %>
       <%  }) %>
    <% } %>
    
    <% if (alerts.success) { %>
        <% alerts.success.forEach(msg => { %>
            <%= msg %>
       <%  }) %>
    <% } %>
</div>